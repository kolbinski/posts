{
  "name": "posts-example",
  "version": "1.0.0",
  "description": "Posts example project",
  "main": "webpack.server.config.js",
  "scripts": {
    "c4mpile": "parallelshell \"npm run compile:client\" \"npm run compile:server\"",
    "compile:prod": "parallelshell \"cross-env NODE_ENV=production npm run compile:client -- -p\" \" cross-env NODE_ENV=production npm run compile:server -- -p\"",
    "compile:watch": "parallelshell \"npm run compile:client:watch\" \"npm run compile:server:watch\"",
    "compile:client": "webpack --config ./webpack/client/index.js",
    "compile:client:watch": "node dev-server.js",
    "compile:server": "webpack --config ./webpack/server/index.js",
    "compile:server:watch": "webpack --watch --config ./webpack/server/index.js",
    "server": "node ./dist/server",
    "server:prod": "cross-env NODE_ENV=production npm run server",
    "server:watch": "nodemon -C --ignore \"*client.js\" --watch ./dist --exec \"npm run server\"",
    "develop": "parallelshell \"npm run compile:watch\" \"npm run server:watch\""
  },
  "author": "Krzysztof Olbinski",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "axios": "^0.14.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.10.2",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-scroll": "^0.2.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classlist-polyfill": "^1.0.3",
    "express": "^4.14.0",
    "react-helmet": "^3.1.0",
    "redial": "^0.5.0",
    "webpack": "^1.13.2"
  }
}
